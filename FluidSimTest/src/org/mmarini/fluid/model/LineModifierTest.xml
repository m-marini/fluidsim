<?xml version="1.0" encoding="UTF-8"?>
<!-- $Id: LineModifierTest.xml,v 1.1 2007/08/18 08:28:29 marco Exp $ -->
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "spring-beans.dtd" >
<beans>
	<bean id="lineModifier"
		class="org.mmarini.fluid.model.LineModifier">
		<property name="cellModifier">
			<bean class="org.mmarini.fluid.model.CellModifierMock"></bean>
		</property>
		<property name="x0" value="0.2" />
		<property name="y0" value="0.2" />
		<property name="x1" value="0.8" />
		<property name="y1" value="0.45" />
	</bean>
	<bean id="universeMock"
		class="org.mmarini.fluid.model.UniverseMock">
		<property name="size">
			<bean class="java.awt.Dimension">
				<constructor-arg value="80" index="0" />
				<constructor-arg value="80" index="1" />
			</bean>
		</property>
	</bean>
</beans>
